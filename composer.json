{
    "name": "Test Unit",
    "description": "Test Unit.",
    "keywords": [
        "php test",
        "jenkins"
    ],
    "license": "MIT",
    "type": "project"
    ,
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phploc/phploc": "^4.0",
        "phpunit/phpunit": "~5.7",
        "sebastian/phpcpd": "^3.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpmd/phpmd" : "@stable"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
